{
  "name": "fansafe",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "npm run build && node -r dotenv/config --inspect out/index.js",
    "start:prod": "node -r dotenv/config out/index.js",
    "start:watch": "nodemon",
    "test-debug": "cross-env TS_NODE_TRANSPILE_ONLY=true mocha --timeout=15000000 --inspect-brk=8804 -r ts-node/register src/**/*.spec.ts --grep @unit",
    "test-integration": "npm run test-type-check && npm run test-integration-no-type-check",
    "test-integration-no-type-check": "cross-env TS_NODE_TRANSPILE_ONLY=true mocha --timeout=15000000 --inspect-brk=8804 -r ts-node/register src/**/*integration.spec.ts --grep @integration",
    "test-type-check": "tsc --noEmit --project ./test-tsconfig.json",
    "build": "tsc --build"
  },
